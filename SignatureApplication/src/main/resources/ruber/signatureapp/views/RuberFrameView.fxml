<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Separator?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>

<BorderPane xmlns:fx="http://javafx.com/fxml/1" style="-fx-background-color: #D9DEE4;" fx:controller="ruber.signatureapp.viewcontrollers.RuberFrameViewController">
    <top>
        <fx:include fx:id="headerView" source="header/HeaderView.fxml"/>
    </top>
    <left>
        <HBox>
            <fx:include fx:id="sessionView" source="session/SessionView.fxml"/>
            <Separator orientation="VERTICAL" />
        </HBox>
    </left>
    <center>
        <fx:include fx:id="selectionView" source="./selection/SelectionView.fxml"/>
    </center>
</BorderPane>